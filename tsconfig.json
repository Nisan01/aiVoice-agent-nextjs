{
  "compilerOptions": {
    "target": "ES2017",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "react-jsx",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    ".next/dev/types/**/*.ts",
    "**/*.mts"
, "app/provider.jsx", "app/layout.jsx", "app/(main)/_components/HeroSection.jsx", "app/(main)/dashboard/_components/LectureHistory.jsx", "app/(main)/dashboard/_components/Feedback.jsx", "app/(main)/discussion-room/[roomId]/page.jsx", "app/api/message/route.js", "app/api/generateSummary/route.js", "app/page.jsx", "app/authProvider.jsx"  ],
  "exclude": ["node_modules"]
}
